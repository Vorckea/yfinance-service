{
    "id": null,
    "title": "YFinance Service Overview",
    "tags": [
        "yfinance",
        "service",
        "monitoring"
    ],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 2,
    "refresh": "10s",
    "panels": [
        {
            "id": 100,
            "type": "stat",
            "title": "Uptime (hours)",
            "datasource": "Prometheus",
            "gridPos": {
                "h": 4,
                "w": 8,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "service_uptime_seconds / 3600",
                    "instant": true,
                    "legendFormat": "uptime_hours"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "h",
                    "decimals": 2
                },
                "overrides": []
            }
        },
        {
            "id": 101,
            "type": "stat",
            "title": "Request Rate (req/s)",
            "datasource": "Prometheus",
            "gridPos": {
                "h": 4,
                "w": 8,
                "x": 8,
                "y": 0
            },
            "targets": [
                {
                    "expr": "sum(rate(request_count_total[1m]))",
                    "instant": true,
                    "legendFormat": "req_s"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "req/s",
                    "decimals": 2
                },
                "overrides": []
            }
        },
        {
            "id": 102,
            "type": "stat",
            "title": "Error Rate (%)",
            "datasource": "Prometheus",
            "gridPos": {
                "h": 4,
                "w": 8,
                "x": 16,
                "y": 0
            },
            "targets": [
                {
                    "expr": "100 * (sum(rate(error_count_total[5m])) / sum(rate(request_count_total[5m])))",
                    "instant": true,
                    "legendFormat": "error_rate_pct"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "decimals": 2,
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "orange",
                                "value": 5
                            },
                            {
                                "color": "red",
                                "value": 10
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "id": 1,
            "type": "timeseries",
            "title": "Request Rate per Endpoint (req/s)",
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 4
            },
            "targets": [
                {
                    "expr": "sum by (endpoint, method) (rate(request_count_total[1m]))",
                    "legendFormat": "{{endpoint}} [{{method}}]"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "req/s"
                },
                "overrides": []
            }
        },
        {
            "id": 2,
            "type": "timeseries",
            "title": "Error Rate per Endpoint (%)",
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 12
            },
            "targets": [
                {
                    "expr": "100 * (sum by (endpoint) (rate(error_count_total[5m])) / sum by (endpoint) (rate(request_count_total[5m])))",
                    "legendFormat": "{{endpoint}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "orange",
                                "value": 5
                            },
                            {
                                "color": "red",
                                "value": 10
                            }
                        ]
                    }
                },
                "overrides": []
            }
        },
        {
            "id": 3,
            "type": "timeseries",
            "title": "P95 Latency per Endpoint (s)",
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 20
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(request_latency_seconds_bucket[5m])) by (le, endpoint))",
                    "legendFormat": "{{endpoint}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "decimals": 3
                },
                "overrides": []
            }
        },
        {
            "id": 4,
            "type": "timeseries",
            "title": "Average Latency per Endpoint (s)",
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 24,
                "x": 0,
                "y": 28
            },
            "targets": [
                {
                    "expr": "sum(rate(request_latency_seconds_sum[5m])) by (endpoint) / sum(rate(request_latency_seconds_count[5m])) by (endpoint)",
                    "legendFormat": "{{endpoint}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "decimals": 3
                },
                "overrides": []
            }
        }
    ]
}